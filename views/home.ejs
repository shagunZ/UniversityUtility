<hr>
      <!-- cards -->

      


    <div class="card container text-center fs-3 mb-4 mt-4" >
      <ul class="list-group list-group-flush" >
        <li class="list-group-item text-danger" >Infrastructure 470+</li>
        <li class="list-group-item text-danger" >Campus Area in Acre 50+</li>
        <li class="list-group-item text-danger" >Placement participation companies 100+</li>
        <li class="list-group-item text-danger" >Industrial Interface 50+</li>
        <li class="list-group-item text-danger" >Seminars/Conferences/Workshop</li>
      </ul>
    </div>


    <div class="container text-center">
      <div class="row">
        <div class="col">
          <div class="card news" style="width: 18rem ;">
            <div class="card-body">
              <h5 class="card-title fs-3">Updates</h5>
              <p class="card-text ">
                <ul >
                  <% for(let u of updates) {%>
                      <div class="container">
                      <li style="font-family:'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif; color: rgb(101, 59, 101);">
                   <b style="border-bottom: 3px solid #dba9fa; color: rgb(173, 17, 129);"> <%=u.date %></b> <br>
                            <p > <%= u.title %>-<%=u.about%> </p>
                           <hr>
                      </li>
                  </div>
                  <% }%>
              </ul>
                </p>
            </div>
          </div>
        </div>
        <div class="col">
          <div class="card news" style="width: 18rem ;">
            <div class="card-body">
              <h5 class="card-title fs-3">Jobs</h5>
              <p class="card-text">
                <ul >
                  <% for(let j of jobs) {%>
                      <div class="container">
                      <li style="font-family:'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif; color: rgb(101, 59, 101);">
                   <b style="border-bottom: 3px solid #dba9fa; color: rgb(173, 17, 129);"><%=j.title%> - <%=j.date %></b> <br>
                            <p > <%= j.about %> </p>
                           <hr>
                      </li>
                  </div>
                  <% }%>
              </ul> 
              </p>
            </div>
          </div>
        </div>
        <div class="col">
          <div class="card news " style="width: 18rem ;">
            <div class="card-body">
              <h5 class="card-title fs-3">Awards and Achievements</h5>
              <p class="card-text">
                <ul>
                  <% for(let a of awards) {%>
                      <div class="container">
                      <li style="font-family:'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif; color: rgb(101, 59, 101);">
                   <b style="border-bottom: 3px solid #dba9fa; color: rgb(173, 17, 129);"><%= a.title %>- <%=a.year %></b> <br>
                            <p ><%-a.about%> </p>
                           <hr>
                      </li>
                  </div>
                  <% }%>
              </ul>
              </p>
            </div>
          </div>
        </div>
        <div class="col">
          <div class="card news " style="width: 18rem;">
              <div class="card-body">
                <h5 class="card-title">Faculty</h5>
                <p class="card-text fs-3">Dr. Ajay Kaul
                  <ul >
                    <% for(let f of faculty) {%>
                        <div class="container">
                        <li style="font-family:'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif; color: rgb(101, 59, 101);">
                     <b style="border-bottom: 3px solid #dba9fa; color: rgb(173, 17, 129);"> <%=f.name %></b> <br>
                              <p > <%= f.experience %>-<%=f.qualification%>
                              <%=f.email%>
                             </p>
                             <hr>
                        </li>
                    </div>
                    <% }%>
                </ul>
                </p>
              </div>
            </div>
      </div>
      </div>
    </div>
<hr>

 

    
